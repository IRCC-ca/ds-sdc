{
  "name": "@ircc-ca/ds-sdc-web",
  "version": "0.0.40",
  "description": "Stencil Component Starter",
  "author": "IRCC-ca",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/web/web.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IRCC-ca/ds-sdc.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "web": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "lint:md": "alex --quiet src/**/*.md && alex --quiet *.md",
    "lint:style": "stylelint src/**/*.scss",
    "lint": "eslint src/**/*{.ts,.tsx} && npm run lint:style"
  },
  "bugs": {
    "url": "https://github.com/IRCC-ca/ds-sdc/issues"
  },
  "publishConfig": {
    "@ircc-ca:registry": "https://registry.npmjs.com/",
    "tag": "ds-sdc-web",
    "directory": "dist"
  },
  "dependencies": {
    "@ircc-ca/ds-sdc-core": "file:../core"
  },
  "devDependencies": {
    "@stencil/angular-output-target": "^0.4.0",
    "@stencil/core": "^2.17.0",
    "@stencil/eslint-plugin": "^0.4.0",
    "@stencil/react-output-target": "^0.3.1",
    "@stencil/sass": "^2.0.0",
    "@types/jest": "^27.0.3",
    "eslint": "^7.0.0",
    "eslint-plugin-react": "7.31.10",
    "jest": "^27.0.3",
    "jest-cli": "^27.4.5",
    "puppeteer": "^19.2.2"
  },
  "license": "MIT"
}
