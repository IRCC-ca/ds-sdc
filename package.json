{
    "name": "@ircc-ca/ds-sdc-sample",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:react": "cd examples/react-stencil && npm run start",
        "build": "ng build",
        "build:packages": "lerna run build --scope=@ircc-ca/ds-sdc-core && lerna run build --scope=@ircc-ca/ds-sdc-web && lerna run build --scope=@ircc-ca/ds-sdc-angular && lerna run build --scope=@ircc-ca/ds-sdc-react",
        "lint": "ng lint && npm run lint:style",
        "format": "lerna run format",
        "web": "lerna run web",
        "lint:md": "alex --quiet sample/**/*.md && alex --quiet *.md",
        "lint:sample:style": "stylelint \"./sample/src/**/*.scss\"",
        "lint:style": "npm run lint:sample:style",
        "test": "lerna run test",
        "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
        "storybook": "ng run sample:storybook",
        "storybook:build": "ng run sample:build-storybook",
        "prepare": "husky install"
    },
    "dependencies": {
        "@angular/animations": "~13.1.0",
        "@angular/common": "~13.1.0",
        "@angular/compiler": "~13.1.0",
        "@angular/core": "~13.1.0",
        "@angular/forms": "~13.1.0",
        "@angular/platform-browser": "~13.1.0",
        "@angular/platform-browser-dynamic": "~13.1.0",
        "@angular/router": "~13.1.0",
        "@ircc-ca/ds-sdc-angular": "file:packages/ds/angular/dist",
        "@ircc-ca/ds-sdc-core": "file:packages/ds/core",
        "@ircc-ca/ds-sdc-web": "file:packages/ds/web",
        "@ngx-translate/core": "^14.0.0",
        "@ngx-translate/http-loader": "^7.0.0",
        "rxjs": "~7.4.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-builders/jest": "^13.0.3",
        "@angular-devkit/build-angular": "~13.1.4",
        "@angular-eslint/builder": "13.2.0",
        "@angular-eslint/eslint-plugin": "13.2.0",
        "@angular-eslint/eslint-plugin-template": "13.2.0",
        "@angular-eslint/schematics": "13.2.0",
        "@angular-eslint/template-parser": "13.2.0",
        "@angular/cli": "~13.1.3",
        "@angular/compiler-cli": "~13.1.0",
        "@babel/core": "^7.17.5",
        "@compodoc/compodoc": "^1.1.18",
        "@storybook/addon-a11y": "^6.5.9",
        "@storybook/addon-actions": "^6.5.9",
        "@storybook/addon-docs": "^6.5.9",
        "@storybook/addon-essentials": "^6.5.9",
        "@storybook/addon-interactions": "^6.5.9",
        "@storybook/addon-links": "^6.5.9",
        "@storybook/angular": "^6.5.9",
        "@storybook/builder-webpack5": "^6.5.9",
        "@storybook/manager-webpack5": "^6.5.9",
        "@storybook/testing-library": "^0.0.13",
        "@types/estree": "^0.0.47",
        "@types/jest": "^27.4.1",
        "@types/node": "^12.11.1",
        "@types/react-dom": "^18.0.5",
        "@typescript-eslint/eslint-plugin": "5.17.0",
        "@typescript-eslint/parser": "5.17.0",
        "alex": "^10.0.0",
        "babel-loader": "^8.2.3",
        "eslint": "^8.12.0",
        "eslint-plugin-storybook": "^0.6.1",
        "husky": "^8.0.1",
        "jest": "^27.5.1",
        "jest-preset-angular": "^11.1.2",
        "lerna": "^4.0.0",
        "ng-packagr": "^13.0.0",
        "prettier": "^2.6.2",
        "storybook-addon-designs": "^6.3.1",
        "storybook-addon-pseudo-states": "^1.15.1",
        "stylelint": "^14.6.1",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recommended-scss": "^6.0.0",
        "stylelint-config-standard": "^25.0.0",
        "stylelint-scss": "^4.2.0",
        "ts-jest": "^27.1.4",
        "typescript": "~4.5.2"
    }
